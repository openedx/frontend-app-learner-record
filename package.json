{
  "name": "@edx/frontend-app-learner-record",
  "version": "0.1.0",
  "description": "Frontend application template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-app-learner-record.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-app-learner-record#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/frontend-app-learner-record/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@1.1.0",
    "@edx/frontend-component-footer": "npm:@edx/frontend-component-footer-edx@^3.1.2",
    "@edx/frontend-component-header": "npm:@edx/frontend-component-header-edx@^5.0.10",
    "@edx/frontend-platform": "1.11.1",
    "@edx/paragon": "13.17.5",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "axios": "^0.21.1",
    "babel-polyfill": "6.26.0",
    "lodash": "^4.17.21",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.4",
    "react-router": "5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "4.1.0"
  },
  "devDependencies": {
    "@edx/frontend-build": "5.6.14",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios-mock-adapter": "^1.19.0",
    "codecov": "3.8.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "es-check": "5.2.4",
    "glob": "7.1.7",
    "husky": "6.0.0",
    "jest": "^27.0.6",
    "jest-react-hooks-shallow": "^1.5.1",
    "react-test-renderer": "^17.0.2",
    "reactifex": "^1.1.1"
  }
}
